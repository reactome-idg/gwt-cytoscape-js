[{
"selector": "node",
	"style": {
	"background-color": "#666",
	"label": "data(id)",
	"shape": "ellipse"
	 }
},
{
  "selector": "node:selected",
  "style": {
    "border-width": "6px",
    "border-color": "#AAD8FF",
    "border-opacity": "0.5",
    "background-color": "#77828C",
    "text-outline-color": "#77828C"
  }
},
{
  "selector": "edge",
  "style": {
    "curve-style": "haystack",
    "haystack-radius": "0.5",
    "opacity": "0.4",
    "line-color": "#bbb",
    "width": "3",
    "overlay-padding": "3px"
  }
},
{
  "selector": "edge:selected",
  "style": {
  	"line-color": "#FF0000"
  }
}]
